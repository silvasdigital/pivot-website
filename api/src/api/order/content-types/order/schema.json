{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "singularName": "order",
    "pluralName": "orders",
    "displayName": "order",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "products": {
      "type": "json",
      "pluginOptions": {}
    },
    "checkout_session": {
      "type": "text",
      "pluginOptions": {}
    },
    "checkout_session_url": {
      "type": "text",
      "pluginOptions": {}
    },
    "customer_email": {
      "type": "string",
      "pluginOptions": {}
    },
    "total": {
      "type": "biginteger",
      "required": true,
      "pluginOptions": {}
    },
    "customer_name": {
      "pluginOptions": {},
      "type": "string"
    },
    "reminder_sent": {
      "pluginOptions": {},
      "type": "boolean",
      "default": false
    },
    "img": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "order_url": {
      "type": "text"
    },
    "error_code": {
      "type": "string"
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "paid",
        "unpaid"
      ],
      "default": "unpaid"
    }
  }
}
